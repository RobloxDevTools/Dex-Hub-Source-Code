local function DIHVF_fake_script() -- Exit.CloseScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		if not (script.Parent.Parent["FASTANIMATIONS?"].BorderSizePixel == 1) then
			script.Parent.Parent.Parent.Parent.Parent["Dex Hub"]:Destroy()
		else
			script.Parent.Parent.Parent.Parent.DragFrame:TweenPosition(
				UDim2.new(0.8,0,2,100),
				"In",
				"Back",
				1,
				false
			)
			wait(1)
			script.Parent.Parent.Parent.Parent.Parent["Dex Hub"]:Destroy()
		end
	end)
end
