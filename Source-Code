local function LDCPNPY_fake_script() -- ESPToggle.ESPToggle 
	local script = Instance.new('LocalScript', ESPToggle)

	local ESPEnabled = false
	script.Parent.CheckFrame.Check.BackgroundColor3 = Color3.fromRGB(170,0,0)
	
	
	function ToggleESP()
		ESPEnabled = not ESPEnabled
		if ESPEnabled then
			script.Parent.CheckFrame.Check.BackgroundColor3 = Color3.fromRGB(0,170,0)
		else
			script.Parent.CheckFrame.Check.BackgroundColor3 = Color3.fromRGB(170,0,0)
		end
		for _, player in pairs(game.Players:GetPlayers()) do
			if player ~= game.Players.LocalPlayer then
				if ESPEnabled then -- Add ESP
					local Box = Instance.new("BoxHandleAdornment")
					Box.ZIndex = 5
					Box.Name = "ESP"
					Box.Size = Vector3.new(4.5, 7, 4.5)
					Box.AlwaysOnTop = true
					Box.Color3 = Color3.fromRGB(255, 0, 0)
					Box.Adornee = player.Character.HumanoidRootPart
					Box.Transparency = 0.5
					Box.Visible = true
					Box.Parent = player.Character.HumanoidRootPart
				else -- Remove ESP
					local ESP = player.Character.HumanoidRootPart:FindFirstChild("ESP")
					if ESP then
						ESP:Destroy()
					end
				end
			end
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		ToggleESP()
	end)
end
coroutine.wrap(LDCPNPY_fake_script)()
