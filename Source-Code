local function AXFA_fake_script() -- FastAnimationsToggle.Aniations Toggle 
	local script = Instance.new('LocalScript', FastAnimationsToggle)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.CheckFrame.Check.BackgroundColor3 == Color3.fromRGB(170, 0, 0) then
			script.Parent.CheckFrame.Check.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
			script.Parent.Parent.Parent.Parent.Parent["FASTANIMATIONS?"].BorderSizePixel = 0
		else
			script.Parent.CheckFrame.Check.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
			script.Parent.Parent.Parent.Parent.Parent["FASTANIMATIONS?"].BorderSizePixel = 1
		end
	end)
end
coroutine.wrap(AXFA_fake_script)()
